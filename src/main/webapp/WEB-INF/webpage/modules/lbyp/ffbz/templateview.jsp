   <script type="text/javascript">
				var list = JSON.parse('${list}');
				if(list.length>0){
      				var tds1 = $("#tr1").find("td");
      				var tds2 = $("#tr2").find("td");
      				tds2.eq(1).html(list[0].jobname.substring(0, 6));
      				var qt = "";
      				for ( var index in list) {
      					var tmp = list[index];
      					if ($.trim(tmp.goodstype) != '3') {
      						for (var i = 0; i < tds1.length; i++) {
      							if ($.trim(tmp.goodsname) == tds1.eq(i).html()) {
      								tds2.eq(i + 2).html(tmp.cyclemonth);
      							}
      						}
      					} else {
      						qt += tmp.goodsname + "" + tmp.cyclemonth + "<br>";
      					}
      				}
      				tds2.eq(tds2.length - 1).html(qt);
				}else{
					$("#tr2").find("td").eq(1).html("无标准");
				}

